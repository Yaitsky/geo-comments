{{#each friends}}
    <li class="friends__item" draggable="true" data-type="friend-item">
        <div class="friends__user">
            <img src="{{photo_100}}" class="friends__photo" alt="User Photo">
            <div class="friends__name">{{first_name}} {{last_name}}</div>
        </div>
        <div class="friends__button">
            <i class="fa fa-plus plus-icon" aria-hidden="true" data-type="add-button"></i>
        </div>
    </li>
{{/each}}
